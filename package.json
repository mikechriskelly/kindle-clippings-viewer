{
  "name": "kindle-clippings-viewer",
  "version": "0.0.1",
  "description": "Simple viewer for Kindle highlights",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "build": "rm build/*; webpack --progress --colors --config ./webpack/webpack-prod.js",
    "dev": "nodemon server/server.js --watch server & webpack-dev-server --config ./webpack/webpack-dev.js"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "Kindle",
    "Clippings",
    "Highlights"
  ],
  "author": "Mike Kelly",
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.3",
    "json-loader": "^0.5.2",
    "kindle-my-clippings": "^0.2.1",
    "react": "^0.13.3",
    "react-tap-event-plugin": "^0.1.7"
  },
  "devDependencies": {
    "babel-core": "^5.5.8",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.1.4",
    "css-loader": "^0.16.0",
    "eslint": "^1.5.0",
    "html-webpack-plugin": "^1.6.1",
    "nodemon": "^1.7.1",
    "react-hot-loader": "^1.2.7",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  }
}
